#!/bin/bash
#PBS -l nodes=1:ppn=8,walltime=4:00:00
#PBS -N tractclassification
#PBS -V

rm -rf tracts

module load singularity 2> /dev/null

echo "starting matlab"
singularity exec -e docker://brainlife/mcr:neurodebian1604-r2017a ./compiled/main

if [ -s output.mat ];
then
	echo 0 > finished
else
	echo "output missing"
	echo 1 > finished
	exit 1
fi


