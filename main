#!/bin/bash
#PBS -l nodes=1:ppn=8,vmem=64gb,walltime=5:00:00
#PBS -N tractclassification
#PBS -V

set -e
set -x

rm -rf tracts

singularity exec -e docker://brainlife/mcr:neurodebian1604-r2017a ./compiled/main

if [ ! -s classification.mat ];
then
	echo "output missing"
	exit 1
fi

mkdir -p wmc
mv classification.mat wmc
mv tracts wmc
